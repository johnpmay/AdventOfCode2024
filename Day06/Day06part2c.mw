<Group hide-input="false" labelreference="L24" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">walkpath := proc(newb)global ogrid, gstart, m, n;local gdir, gpos, grid, count, gpath, gnew;gdir := [-1, 0]; # starting orientation ^
gpos := gstart;
grid := Array(1..m,1..n,()-&gt;&quot;.&quot;):
grid[1..m,1..n] := ogrid:
if grid[newb[]] = &quot;#&quot; or newb=gstart then error &quot;already&quot;, newb; end if;grid[newb[]] := &quot;#&quot;;count := 0;
gpath := table();
for count to 12*n^2 while map2(clamp, 1, gpos,m) = gpos do 
    gnew := gpos +~ gdir;
    if map2(clamp, 1, gnew, m) &lt;&gt; gnew then return false; break; end if; # off grid    if grid[gnew[]]=&quot;#&quot; then
    # rotate, don't step yet
        gdir := rot90(gdir);
    else
        gpos := gnew;
        if gpath[gpos, gdir] = true then break; end if; # looped        gpath[gpos, gdir] := true;
    end if;    
end do:#printf(&quot;%a=%a, &quot;,count,newb);return evalb( map2(clamp, 1, gpos,m) = gpos );end proc:</Text-field>
</Input>
</Group>
