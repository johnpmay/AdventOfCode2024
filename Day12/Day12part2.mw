<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Regions := table(sparse = {}):Tracking:=convert(grid, table):for start in allstart do    if Tracking[start[]] = &quot;.&quot; then        next;    else        Tracking[start[]] := &quot;.&quot;;    end if;    nS := DEQueue([start]);rg := DEQueue(); # resultwhile not empty(nS) do    curr := pop_back(nS);# find boundries    bdd := {seq}(ifelse(grid[curr[]]=grid[(curr+d)[]],NULL,d), d in [ [0,1],[1,0],[0,-1],[-1,0] ]);    if   nops(bdd) = 4 then corn := seq(seq([i,j],i=0..1),j=0..1);    elif nops(bdd) = 3 then         if not [ 0, 1] in bdd then corn := [0,0],[1,0];      elif not [-1, 0] in bdd then corn := [1,0],[1,1];      elif not [ 1, 0] in bdd then corn := [0,0],[0,1];      elif not [ 0,-1] in bdd then corn := [0,1],[1,1];       end if;    elif nops(bdd) &lt;= 1 then corn := NULL;    elif bdd[1]*~bdd[2] = [0,0] then        if bdd = {[ 1, 0],[ 0, 1]} then corn := [1,1];
      elif bdd = {[-1, 0],[ 0,-1]} then corn := [0,0];
      elif bdd = {[ 1, 0],[ 0,-1]} then corn := [1,0];      elif bdd = {[-1, 0],[ 0, 1]} then corn := [0,1];      end if;    else corn := NULL;    end if;# find other direction corners    if [0,1] in bdd and not [-1,0] in bdd                    and grid[(curr+[-1,1])[]] = grid[curr[]]    then corn ,= [0,1];    end if;    if [-1,0] in bdd and not [0,-1] in bdd
                     and grid[(curr+[-1,-1])[]] = grid[curr[]]
    then corn ,= [0,0];
    end if;    if [0,-1] in bdd and not [1,0] in bdd
                    and grid[(curr+[1,-1])[]] = grid[curr[]]
    then corn ,= [1,0];
    end if;    if [1,0] in bdd and not ([0,1] in bdd)
                    and grid[(curr+[1,1])[]] = grid[curr[]]
    then corn ,= [1,1];
    end if;
    push_back(rg, [curr, bdd, ({seq(curr+c,c in {corn})})]);        for dir in alldir do        if Tracking[(curr+dir)[]] = grid[curr[]] then            Tracking[(curr+dir)[]] := &quot;.&quot;;            push_back(nS, curr+dir);        end if;    end do;end do:Regions[grid[start[]]] union= {convert(rg, list)};end do:cost := table(sparse=0):cost2 := table(sparse=0):tot := 0:tot2:=0:for v in allveg do   for r in Regions[v] do      perim := add(nops(s[2]), s in r);      corns := nops([seq(s[3][], s in r)]);      area := nops(r);      cost[v] += perim*area;      cost2[v] += corns*area;   end do;   tot += cost[v];   tot2 += cost2[v];end do:#entries(cost2, pairs);ans1 := tot;ans2 := tot2;    </Text-field>
