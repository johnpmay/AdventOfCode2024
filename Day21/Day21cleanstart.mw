    if nops(L[1]) = 0 then return [] end if;
    if nops(L) = 1 then return [seq([l], l in L[1])]; end if;
    rets := thisproc(L[2..-1]);
    return [seq(seq([l,r[]], r in rets), l in L[1])];
end proc:</Text-field>
