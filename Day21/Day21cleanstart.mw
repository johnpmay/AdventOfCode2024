<Group labelreference="L4" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">NShortest := proc(s, e)
local d, m, i;
global Npaths;
   if type(Npaths[s,e],set) then return Npaths[s,e]; end if;
   d := lclamp(-1,rngrid[e] - rngrid[s],1);
   if s = e then return {[s]};
   elif rngrid[s][1]=rngrid[e][1] or rngrid[s][2]=rngrid[e][2] then 
       return {[s,seq(ngrid[rngrid[s]+i*d],i=1..max(abs~(rngrid[e]-rngrid[s])))]}; # unique
   elif rngrid[s][1]=4 and rngrid[e][2] = 1 then # vfirst
       return { [s,
           seq(ngrid[rngrid[s]+i*[d[1],0]],i=1..abs~(rngrid[e][1]-rngrid[s][1])),
           seq(ngrid[[rngrid[e][1],rngrid[s][2]]+i*[0,d[2]]],i=1..abs~(rngrid[e][2]-rngrid[s][2]))
                   ] };
   elif rngrid[e][1]=4 and rngrid[s][2] = 1 then # h first
       return { [s,
           seq(ngrid[rngrid[s]+i*[0,d[2]]],i=1..abs~(rngrid[e][2]-rngrid[s][2])),
           seq(ngrid[ [rngrid[s][1],rngrid[e][2]]+i*[d[1],0]],i=1..abs~(rngrid[e][1]-rngrid[s][1]))
              ]};
   else # two choices     
      return { [s,
           seq(ngrid[rngrid[s]+i*[d[1],0]],i=1..abs~(rngrid[e][1]-rngrid[s][1])),
           seq(ngrid[[rngrid[e][1],rngrid[s][2]]+i*[0,d[2]]],i=1..abs~(rngrid[e][2]-rngrid[s][2]))],
         [s,
           seq(ngrid[rngrid[s]+i*[0,d[2]]],i=1..abs~(rngrid[e][2]-rngrid[s][2])),
           seq(ngrid[ [rngrid[s][1],rngrid[e][2]]+i*[d[1],0]],i=1..abs~(rngrid[e][1]-rngrid[s][1]))]
       };
   end if;
   error &quot;SNH&quot;;
end proc:</Text-field>
</Input>
</Group>
