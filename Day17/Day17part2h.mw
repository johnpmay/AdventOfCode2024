#PRINT(&quot;%d : %s %d\134n&quot;, instr, opname[</Font>program[instr+1]+1] , program[instr+2]);
    opcode[program[instr+1]](program[instr+2]);
end do:
#PRINT(&quot;HALT&quot;);
instr = endloc;

return convert(out,list);
end proc:

out := runprogram(program, reg);
ans1:=Join(map(convert, out, string),&quot;,&quot;);
