<Group hide-input="false" labelreference="L24" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">walkpath := proc(newb, start:=gstart, dir:=[-1,0], pathsofar:=table(),$)
local gdir, gpos, grid, count, gpath, gnew;

gdir := dir; grid := ogrid: gpos := start;
grid[newb[]] := 1; # set barrier
count := 0;
gpath := copy(pathsofar);
for count to n^2 do 
    gnew := gpos +~ gdir;
    if map2(clamp, 1, gnew, m) &lt;&gt; gnew then
        # off grid
        gpos := gnew;
        break;
    elif grid[gnew[]]=1 then
        # rotate, don't step yet
        gdir := rot90(gdir);
    else
        gpos := gnew;
        if gpath[gpos, gdir] = true then
            
            break;
        end if;
        gpath[gpos, gdir] := true;
    end if;    
end do:
grid[newb[]] := 0; # remove barrier
return evalb( map2(clamp, 1, gpos,m) = gpos );
end proc:</Text-field>
</Input>
</Group>
