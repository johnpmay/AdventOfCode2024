runprogram := proc(program, reg)
global instr, register, out;
local endloc;
register := table(reg);
instr := 0;
endloc := nops(program);
out := DEQueue():

while instr &lt; endloc do
#&quot;i&quot;=instr, &quot;o&quot;=program[instr+1], &quot;n&quot;=program[instr+2];

