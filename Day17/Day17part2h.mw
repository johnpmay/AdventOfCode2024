<Group hide-input="false" labelreference="L14" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">litop := proc(n) return n end proc:

combo := proc(n) 
global register;
    if n&lt;=3 then
       return n; 
    elif n&lt;=6 then
       return register[n-3];
    else
       error;
    end if;
end proc:

opcode[0] := proc(n)
global register, instr;
   instr += 2;
   <Font encoding="UTF-8">PRINT(&quot;A = %d / %d\134n&quot;, </Font>register[1],  2^combo(n) );
   register[1] := trunc( register[1] / 2^combo(n));
end proc: # 0 adv - division

opcode[1] := proc(n)
global register, instr;
   instr += 2;
<Font encoding="UTF-8">   PRINT(&quot;B = %d XOR %d\134n&quot;, </Font>register[2], litop(n) );
   register[2] := Bits:-Xor( register[2], litop(n) );
end proc: # 1 bxl - xor

opcode[2] := proc(n)
global register, instr;
    instr += 2;
    <Font encoding="UTF-8">PRINT(&quot;B = %d =&gt; %d\134n&quot;, </Font>combo(n), combo(n) mod 8);
    register[2] := combo(n) mod 8;
end proc: # bst - low 3 bits

opcode[3] := proc(n)
global register, instr;
    if register[1] = 0 then
        <Font encoding="UTF-8">PRINT(&quot;Don't jump, register A=0\134n&quot;);</Font>
        instr += 2;
    else
<Font encoding="UTF-8">        PRINT(&quot;Jump to %d, register A=%d\134n&quot;, litop(n), register[1]);
</Font>        instr := litop(n);
    end if;
end proc: # jnz - jump non-zero

opcode[4] := proc(n)
global register, instr;
    instr += 2;
<Font encoding="UTF-8">    PRINT(&quot;B = %d XOR %d\134n&quot;, </Font>register[2], register[3] );
    register[2] := Bits:-Xor( register[2], register[3] ); # ignore n
end proc: # bxc

opcode[5] := proc(n)
global register, instr, out;
    instr += 2;
    <Font encoding="UTF-8">PRINT(&quot;OUTPUT %d =&gt; %d\134n&quot;, </Font>combo(n), combo(n) mod 8);
    push_back(out, combo(n) mod 8);
end proc: # out

opcode[6] := proc(n)
global register, instr;
    instr += 2;
