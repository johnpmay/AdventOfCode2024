<Group hide-input="false" labelreference="L29" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">ttt := time[real]():
gdir := [-1, 0]; # starting orientation up ^
gpos := gstart;
goods := {}:
npath := table():
npath[gpos, gdir] := true: # track the path for walkpath shortcut
for count from 0 to n^2 do # m=n
    gnew := gpos +~ gdir;
    if map2(clamp, 1, gnew,m) &lt;&gt; gnew then break; end if;
    if grid[gnew[]]=1 then
        gdir := rot90(gdir);
    else
        if gnew &lt;&gt; gstart and           # KEY: if we check gnew already, don't check it again - it could succeed from the second visit
           not orseq(evalb(npath[gnew,d]=true), d in {[1,0],[0,1],[-1,0],[0,-1]}) and            not gnew in goods and
           walkpath(gnew, gpos, gdir, npath)
        then
           goods := goods union {gnew};
        end if;
        gpos := gnew;
        npath[gpos, gdir] := true;
    end if;
#if count mod 500 = 0 then printf(&quot;%d, &quot;,count); end if; # &quot;progress bar&quot;
end do:
ans2b := nops(goods);
&quot;time&quot;=(time[real]()-ttt) *Unit(seconds);</Text-field>
