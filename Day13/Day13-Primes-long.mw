src := sprintf(&quot;proc(m)
local asol := %a;
local bsol := %a;
local deno := %a;
if deno = 0 then return -2^63; end if;
if asol mod deno = 0 and bsol mod deno = 0
   and (   ( deno&gt;=0 and asol&gt;=0 and bsol&gt;=0 ) 
        or ( deno&lt;=0 and asol&lt;=0 and bsol&lt;=0 ) )
then
    return 3*asol/deno+bsol/deno;
else
    return 0;
end if;
end proc&quot;, numer(symsol[1]), numer(symsol[2]), denom(symsol[1])):
solveit := parse(src);

tokens := 0:
for m in machines do
    tokens := tokens + solveit(m);
end do:

ans1 = tokens;</Text-field>
